{
    "homepage": "https://sourceforge.net/projects/goldendict/",
    "description": "A feature-rich dictionary lookup program.",
    "notes": "Modified by nonwill, https://nonwill.github.io/post/goldendict/",
    "version": "2020.05.06",
    "license": "freeware",
    "url": "https://forum.freemdict.com/uploads/short-url/xQF2Hy027fZE0TyMMGI52h5TrDY.7z",
    "hash": "fe12d75daa6a9e8d2f117b18b0c403bfcc2eda69935a109bee1316a3daa7f84a",
    "extract_dir": "",
    "bin": "GoldenDict.exe",
    "shortcuts": [["GoldenDict.exe", "GoldenDict"]],
    "persist": "portable",
    "post_install": [
        "# update bin",
        "git clone --depth=1 https://github.com/nonwill/nwDeployed $dir\\nwDeployed",
        "Copy-Item -Path \"$dir\\nwDeployed\\bin5_x64_msvc\\*\" -Destination \"$dir\" -Recurse -Force -Confirm:$false",
        "Remove-Item -Path \"$dir\\nwDeployed\" -Recurse -Force -Confirm:$false",
        "# ocr",
        "curl -L -o \"$dir\\ocr_x64.7z\" \"https://forum.freemdict.com/uploads/short-url/tSYVG8Ie6gxbAXF9nIXN4S7yNfl.7z\"",
        "if ($?) {7z.exe x \"$dir\\ocr_x64.7z\" \"-o$($dir)\" -y > $null}",
        "Remove-Item -Path \"$dir\\ocr_x64.7z\" -Recurse -Force -Confirm:$false"
    ],
    "checkver": {
        "url": "https://forum.freemdict.com/t/topic/199/1",
        "re": "【([\\d.]+)】\\s*?GoldenDict"
    },
    "autoupdate": {
        "url": "https://codeload.github.com/nonwill/nwDeployed/zip/master",
        "extract_dir": "nwDeployed-master\\bin5_x64_msvc"
    }
}
